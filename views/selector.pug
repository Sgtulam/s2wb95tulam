extends layout

block content
  h1= title
  p.
    Welcome to #{title}
  - var image_names=["mag","gold","nik","cop","iron"]
  - for(var i=0;i<3;i++){
      - var name=image_names[Math.floor(Math.random()*5)]
        img(src=`../../public/images/${name}.jpg`, alt="image" height="200", width="200")
  - }
    